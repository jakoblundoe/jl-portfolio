.block {
  display: block;
}
body {
    margin: 0;
    padding: 0;
}
a, p, h1, h2, h3, h4, h5, h6 {
    text-decoration: none;
    font-family: 'Inter', sans-serif;
    font-weight: 400;
    letter-spacing: 0.5px;
    margin: 0;
    padding: 0;
}
.default_page {
    min-height: 100dvh;
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-between;

    background: linear-gradient(359.91deg, rgba(255, 255, 255, 0.06) 0.07%,
                                           rgba(138, 138, 138, 0.12) 65.47%,
                                           rgba(88, 88, 88, 0.1) 82.95%,
                                           rgba(54, 54, 54, 0.2) 99.92%),
                                           #000000;
    background-blend-mode: normal, soft-light;
    mix-blend-mode: normal;
}
.index_page, .showreel_page {
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-between;

    min-height: 100dvh;

    background-image: url('/img/pressphoto-jln.jpeg');
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
}
.mainviewport {
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-evenly;
    align-items: center;
}
.videocontent {
    display: flex;
    justify-content: center;
    align-items: center;
    margin: auto 0 auto 0;
}
.videocontent video {
    width: min(85vw, 90%);
    height: auto;
    max-width: 1200px;
    max-height: auto;
    border-radius: 2px;
    box-shadow: 1px 1px 4px #000000;

    margin: 50px 0 34px 0;
}
.footer {
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;

    padding: 0px 28px 0px 20px;
    
    background: #FFFFFF;
}
.somelinkcontainer {
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    align-items: center;
    padding: 3px 0 3px 24px;
    gap: 6px;
}
.somelinkcontainer .someitem {
    transition:
    all 140ms;
}
.somelinkcontainer .someitem:hover {
    opacity: .7;
    transform: scale(1.05);
}
.somelinkcontainer .someitem:active {
    opacity: .5;
    transform: scale(1.05);
}
#linkedin-icon {
    width: 36px;
    height: auto;
}
#instagram-icon {
    width: 28px;
    height: auto;
}
#github-icon {
    width: 50px;
    height: auto;
    margin-right: 2px;
}
.navbar {
    display: flex;
    flex-flow: row nowrap;
    justify-content: flex-start;
    align-items: center;

    gap: 18px;
    margin-right: -4px;

    min-height: 56px;
}
.navbar .logocontainer {
    display: flex;
    width: 50px;
    height: auto;
    align-items: center;
    justify-content: center;
    opacity: .8;

    transition:
    all 100ms;
}
.navbar .logocontainer:hover {
    opacity: .7;
    transform: scale(1.05);
}
.navbar .logocontainer:active {
    opacity: .5;
    transform: scale(1.05);
}
.navbar .logo {
    max-width: 100%;
    max-height: 100%;
    width: 32px;
}
.navbar .navbaritemcontainer {
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
    align-items: center;
    padding: 4px 6px;
    gap: 30px;
}
.navbaritemcontainer a {
    font-family: 'Roboto Mono', monospace;
    font-weight: 400;
    letter-spacing: 0.2px;

    font-size: 19px;

    color: #262626;
    text-shadow: 0px 2px 10px rgba(0, 0, 0, 0.2);

    transition:
    all 100ms;
}
.navbaritemcontainer a:hover {
    text-shadow: 1px 4px 10px rgba(0, 0, 0, 0.2);
    /* transform: scale(1.01); */
    opacity: .75;
}
.navbaritemcontainer a:active {
    text-shadow: 1px 2px 4px rgba(0, 0, 0, 0);
    opacity: .6;
}
.navbaritemcontainer .activepage {
    text-decoration: underline;
}
.header {
    padding: 2px 24px;
    gap: 17px;
}
.header .textcontainer {
    display: flex;
    flex-flow: column nowrap;

    padding: 4px 0px;
}
.header h1 {
    color: #FFFFFF;
    text-shadow: 1px 2px 8px #000000;

    font-size: 40px; /* fallback font-size */
    font-size: min(8vw, 50px);
}
.header h2 {
    line-height: 29px;
    color: #FFFFFF;
    text-shadow: 1px 2px 8px #000000;
    
    font-size: 24px; /* fallback font-size */
    font-size: min(4.28vw, 30px);
}
.showreelbuttoncontainer {
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 30px 0 34px 0;
}
.showreelbutton {
    box-sizing: border-box;

    padding: 18px 30px;
    gap: 21px;
    border: 2px solid #FFFFFF;
    background-color:#2424243d;
    filter: drop-shadow(1px 1px 4px #000000);

    transition:
    all 200ms;
}
.showreelbutton p {
    font-family: 'Roboto Mono', monospace;
    font-size: 20px;
    line-height: 26px;

    color: #FFFFFF;

    text-shadow: 2px 4px 8px #000000;

    transition:
    all 200ms;
}
.showreelbutton:hover {
    background: #141414b6;
    border: 2px solid #FFFFFF;
    box-shadow: 1px 1px 4px #000000;

    transform: scale(1.03);
}
.showreelbutton:hover p {
    font-weight: 900;
    font-size: 20px;
    line-height: 26px;
    
    color: #ffffff;
    
    text-shadow: 2px 4px 8px #000000;
}
.showreelbutton:active {
    background: #444444;
    border: 2px solid #FFFFFF;
    box-shadow: 0px 0px 0px #000000;

    transform: scale(1.02);
}
.showreelbutton:active p {
    font-weight: 900;
    font-size: 20px;
    line-height: 26px;
    
    color: #ffffff;
    
    text-shadow: 0px 0px 0px #000000;
}
.contactcontent {
    display: flex;
    flex-flow: column nowrap;
    align-self: center;
    gap: 40px;

    margin: 80px 0px 140px 0px;
}
.contact_page {
    min-height: 100dvh;
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-between;

    background: linear-gradient(359.91deg, rgb(255, 255, 255) 0.07%,
    rgb(255, 255, 255) 65.47%,
    rgb(255, 255, 255) 82.95%,
    rgb(255, 255, 255) 99.92%),
    #ffffff;
    background-blend-mode: normal, soft-light;
    mix-blend-mode: normal;
}
.contact_page .header, .about_page .header {
    position: relative;
    /* border-bottom: 1px solid rgb(165, 165, 165); */
}
.contact_page .header::after, .about_page .header::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 25px; /* Adjust to control the starting point */
    right: 25px; /* Adjust to control the ending point */
    border-bottom: 1px solid rgb(165, 165, 165);
}
.contact_page .header h1, .contact_page .header h2 {
    color: #3f3f3f;
    /* text-shadow: 0.5px 1px 2px #0000005b; */
    text-shadow: none;
}
.contact_page .header h1 {
    font-size: 30px; /* fallback font-size */
    font-size: min(8vw, 37.5px);
}
.contact_page .header h2 {
    font-size: 18; /* fallback font-size */
    font-size: min(4.28vw, 22.5px);
}
.contact_page .footer, .about_page .footer {
    border-top: 1px solid #000000;
}
.contacttitle {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: flex-start;
    padding: 0px;
    gap: 10px;
}
.contacttitle p {
    font-weight: 500;
    font-size: 54px;
    line-height: 24px;
    letter-spacing: 3px;
    text-align: center;

    color: #252525;

    /* text-shadow: 1px 2px 8px #000000; */
}
.emailcontainer {
    display: flex; 
    flex-flow: column nowrap;
    align-self: center;
    padding: 0px;

    gap: 10px;
}
.buttoncontainer {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 0px;
    margin-top: 34px;
    gap: 10px;
}
.emailbutton {
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
    align-items: center;
    padding: 30px 36px;
    gap: 10px;

    /* background: #292929; */
    border: 2px solid #e33c72;
    border-radius: 7px;
    text-shadow: 0.5px 4px 1px #000000;

    transition:
    all 300ms;
}
.buttoncontainer a {
    font-weight: 400;
    font-size: 30px;
    line-height: 17px;
    letter-spacing: 1px;
    color: #e33c72;
    text-shadow: 0.5px 1px 1px #000000b9;

    transition:
    all 300ms,
    opacity 100ms;
}
.emailbutton:hover {
    scale: 1.01;
}
.emailbutton:active {
    opacity: 70%;
    transition:
    opacity 100ms;
}
.contactinfo {
    display: flex;
    align-self: center;
    padding: 4px;
    gap: 10px;
}
.contactinfo p {
    font-weight: 300;
    font-size: 24px;
    letter-spacing: 1px;
    line-height: 17px;
    /* identical to box height */
    text-align: center;

    color: #252525;
}
.about_page {
    min-height: 100dvh;
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-between;

    background: white;
    background-blend-mode: normal, soft-light;
    mix-blend-mode: normal;
}
.about_page a, .about_page p, .about_page h1, .about_page h2,
.about_page h3, .about_page h4, .about_page h5, .about_page h6 {
    color: #3f3f3f;
    text-shadow: none;
    letter-spacing: 0.1px;
}
.about_page h1 {
    font-size: 30px; /* fallback font-size */
    font-size: min(8vw, 37.5px);
}
.about_page h2 {
    font-size: 18; /* fallback font-size */
    font-size: min(4.28vw, 22.5px);
}
.aboutcontentcontainer {
    display: flex;
    flex-flow: row wrap;
    justify-content: center;
    gap: 60px;
    margin: 30px 35px;
}
.aboutdescription {
    display: flex;
    flex-flow: column nowrap;
    justify-content: flex-start;
    align-items: center;
    width: 400px;
    gap: 10px;
}
.aboutdescription .biotitle {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
}
.biotitle h2 {
    font-weight: 600;
    font-size: 36px;
    text-align: center;
    letter-spacing: 2px;
}
.aboutdescription .biosubtitle {
    display: flex;
    flex-direction: row;
    align-items: flex-start;
    gap: 10px;
}
